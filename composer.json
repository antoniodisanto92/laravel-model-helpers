{
    "name": "antoniodisanto92/model-helpers",
    "description": "A set of helpers for your Eloquent Models",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio D'Isanto",
            "email": "antoniodisanto92ds@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Antoniodisanto92\\ModelHelpers\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.46"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antoniodisanto92\\ModelHelpers\\ModelHelpersServiceProvider"
            ],
            "aliases": {
                "ModelHelpers": "Antoniodisanto92\\ModelHelpers\\Facades\\ModelHelpers",
                "MacroableModels": "Antoniodisanto92\\ModelHelpers\\Facades\\MacroableModels"
            }
        }
    }
}
